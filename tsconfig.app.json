{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/**/*.ts"],
  "exclude": [
    "src/**/__tests__/*",
    "../../node_modules",
    "../../typings"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/**"]
    },
    "typeRoots": ["../../typings", "../../node_modules/@types"]
  }
}
